caliproject.duckdns.org, localhost {
    encode zstd gzip
    
    @api path /api/* /static/* /health
    
    route {
        reverse_proxy @api backend:8080
        root * /srv
        try_files {path} /index.html
        file_server
    }
    
    header /assets/* Cache-Control "public, max-age=31536000, immutable"
}